<h1>Editing Book</h1>

<%= form_with model:  @books, url: update_books_path(@books.id), method: :patch do |f| %>

 <h4>Title</h4>
 <%= f.text_area :title %>
 
 <h4>Body</h4>
 <%= f.text_area :body %>
 
 <%= f.submit 'Update Book' %>
 
 <%= link_to "Show", books_path(book.id) %>
 "|"
 <%= link_to "Back", books_path %>
 
<% end %>